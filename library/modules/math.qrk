(declare (+ a) (-> a a a))
(defn + [a b] (binary a "+" b))

(declare (- a) (-> a a a))
(defn - [a b] (binary a "-" b))

(declare (* a) (-> a a a))
(defn * [a b] (binary a "*" b))

(declare (/ a) (-> a a a))
(defn / [a b] (binary a "/" b))

(declare (< a) (-> a a bool))
(defn < [a b] (binary a "<" b))

(declare (> a) (-> a a bool))
(defn > [a b] (binary a ">" b))

(defn . [f g]
  (fn [x] (f (g x))))