; CORE DECLARATIONS
(declare (print a) (-> a int))
(declare (begin a) (-> [a] a))
(declare (extern a) (-> str a))

(declare (Call a b) (-> a a b))
(declare (Property a b) (-> a a b))
(declare (Var b) (-> str b))

(let print (fn [x]
  (Call (Property (Var "console") (Var "log")) x)))

; ARITHMETIC OPERATORS
(declare (+ a) (-> a a a))
(declare (- a) (-> a a a))
(declare (* a) (-> a a a))
(declare (/ a) (-> a a a))

(declare (= a) (-> a a bool))