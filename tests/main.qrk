(data (Maybe a))
(declare (Just a) (-> a (Maybe a)))
(declare (Nothing a) (Maybe a))

(data (List a))
(declare (Cons a) (-> a (List a) (List a)))
(declare (Nil a) (List a))

(data (IO a))

(declare (print a) (-> a (IO a)))
(declare (main a) (-> (List str) (IO a)))

(let main (fn [args] (print (Cons 5 Nil))))